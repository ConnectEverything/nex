{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "io.nats.nex.v2.agent_heartbeat",
  "title": "AgentHeartbeat",
  "type": "object",
  "properties": {
    "state": {
      "type": "string",
      "description": "The state of the agent. Must be of able to map to models.AgentState"
    },
    "workload_count": {
      "type": "integer",
      "description": "The number of agents running with workload count"
    },
    "data": {
      "type": "string",
      "description": "Send additional data in heartbeat"
    }
  },
  "required": [
    "state",
    "workload_count",
    "data"
  ],
  "additionalProperties": false
}
