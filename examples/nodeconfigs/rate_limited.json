{
    "kernel_filepath": "/path/to/vmlinux-5.10",
    "rootfs_filepath": "/path/to/rootfs.ext4",
    "machine_pool_size": 1,
    "cni": {
        "network_name": "fcnet",
        "interface_name": "veth0"
    },
    "machine_template": {
        "vcpu_count": 1,
        "memsize_mib": 256
    },
    "rate_limiters": {
        "bandwidth": {
            "one_time_burst": 1048576,
            "refill_time": 500,
            "size": 1048576
        },
        "iops": {
            "one_time_burst": 100,
            "refill_time": 1000,
            "size": 100
        }
    }
}
